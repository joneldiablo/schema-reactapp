  "routes": {
    "template": "$templates.admin",
    "children": [
      {
        "path": "/",
        "exact": true,
        "content": "$pages.nav-main"
      },
      {
        "template": {
          "ref": "$templates.side-panel",
          "content": [
            {
              "ref": "$sections.nav-side",
              "className": "bg-light shadow h-100",
              "attributes": {
                "menu": "$menus.main"
              }
            },
            {
              "ref": "$sections.container",
              "allowChildren": true,
              "className": "h-100",
              "attributes": {
                "id": "table-container",
                "responsive": true,
                "className": "h-100"
              }
            }
          ]
        },
        "transition": "slideRight",
        "children": [
          {
            "path": "/catalogo",
            "children": [
              {
                "path": "/",
                "content": "$pages.catalog",
                "exact": true
              },
              {
                "template": {
                  "ref": "$templates.side-panel",
                  "content": [
                    {
                      "ref": "$sections.nav-service",
                      "className": "bg-light shadow h-100 overflow-auto",
                      "attributes": {
                        "url": "https://my.api.mockaroo.com/products.json?key=30283510",
                        "iconFrom": "gallery",
                        "labelFrom": "name"
                      }
                    },
                    {
                      "ref": "$sections.container",
                      "allowChildren": true,
                      "className": "h-100",
                      "style": {
                        "position": "relative"
                      }
                    }
                  ]
                },
                "children": [
                  {
                    "path": "/:itemPath",
                    "content": "$pages.item"
                  }
                ]
              }
            ]
          },
          {
            "path": "/pedidos",
            "content": "$pages.orders"
          },
          {
            "path": "/descuentos",
            "content": "$pages.discounts"
          },
          {
            "path": "/configuracion",
            "children": [
              {
                "path": "/",
                "content": "$pages.nav-config",
                "exact": true
              },
              {
                "template": {
                  "ref": "$templates.side-panel",
                  "content": [
                    {
                      "ref": "$sections.nav-side",
                      "className": "bg-light shadow h-100",
                      "attributes": {
                        "menu": "$menus.config"
                      }
                    },
                    {
                      "ref": "$sections.container",
                      "allowChildren": true
                    }
                  ]
                },
                "children": [
                  {
                    "path": "/usuario",
                    "content": "$pages.user"
                  },
                  {
                    "path": "/plantilla",
                    "content": "$pages.template"
                  },
                  {
                    "path": "/tema",
                    "content": "$pages.theme"
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  },
  "themes": {
    "easy-commerce": {
      "base": "default",
      "label": "Easy Commerce bs theme",
      "style": "",
      "variables": {},
      "fonts": []
    }
  }
