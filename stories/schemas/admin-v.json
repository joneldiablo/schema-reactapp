{
  "site": "Vetement",
  "description": "Ropa para toda la familia",
  "icon": "icon.ico",
  "logo": "logo1.png",
  "domain": "/",
  "api": "api/",
  "theme": "$themes.easy-commerce",
  "assets": {
    "images": "assets/images",
    "icons": "assets/icons",
    "videos": "assets/videos",
    "audios": "assets/audios",
    "fonts": "assets/fonts",
    "files": "assets/files"
  },
  "sections": {
    "header": {
      "type": "navbar",
      "label": "Header",
      "description": "Header of site, always at top.",
      "icon": "navbar",
      "attributes": {
        "logo": "$logo",
        "logoHeight": 80,
        "description": "$description",
        "menu": "$menus.user",
        "menuLeft": "$menus.left",
        "menuRight": "$menus.right"
      }
    },
    "container": {
      "type": "container",
      "label": "Container",
      "description": "Wrap content of page.",
      "icon": "content"
    },
    "nav-side": {
      "type": "nav-side",
      "label": "Side Menu",
      "description": "menu of routes.",
      "icon": "menu"
    },
    "nav-service": {
      "type": "nav-service",
      "label": "Side Menu from service",
      "description": "menu of routes provided by a service.",
      "icon": "menu"
    },
    "footer": {
      "type": "footer",
      "label": "Footer",
      "description": "Footer of site, always at bottom.",
      "icon": "footer",
      "attributes": {
        "menu": "$menus.footer"
      }
    },
    "hero": {
      "label": "Hero",
      "type": "hero",
      "description": "First impresive image or banner container, wellcome to the site."
    },
    "signin-form": {
      "type": "form",
      "label": "Sign In",
      "attributes": {
        "endpoint": "",
        "fields": {
          "user": {},
          "password": {},
          "remember-me": {}
        }
      }
    },
    "signup-form": {
      "type": "form",
      "label": "Sign Up",
      "attributes": {
        "endpoint": "",
        "fields": {
          "user": {},
          "password": {}
        }
      }
    },
    "jexcel": {
      "type": "table-jexcel"
    },
    "nav-cards": {
      "type": "nav-cards",
      "label": "navication cards",
      "description": "menu of routes on cards.",
      "icon": "menu"
    }
  },
  "menus": {
    "main": [
      {
        "ref": "$pages.catalog"
      },
      {
        "ref": "$pages.discounts"
      },
      {
        "ref": "$pages.orders"
      },
      {
        "ref": "$pages.nav-config"
      }
    ],
    "config": [
      {
        "ref": "$pages.user"
      },
      {
        "ref": "$pages.template"
      },
      {
        "ref": "$pages.theme"
      }
    ],
    "user": [
      {
        "ref": "$pages.user",
        "path": "/configuracion/usuario",
        "exact": true,
        "label": "Administrador"
      }
    ]
  },
  "templates": {
    "admin": {
      "type": "template",
      "label": "Admin",
      "description": "template for admin",
      "icon": "template",
      "image": "template.jpg",
      "className": "d-flex flex-column h-100 w-100",
      "style": {
        "position": "absolute"
      },
      "content": [
        {
          "ref": "$sections.header",
          "className": "bg-light shadow-sm",
          "style": {
            "zIndex": 4
          },
          "attributes": {
            "centeredLogo": false,
            "menuPosition": "right",
            "textOverColor": "light"
          }
        },
        {
          "ref": "$sections.container",
          "allowChildren": true,
          "className": "h-100",
          "style": {
            "position": "relative"
          }
        },
        {
          "ref": "$sections.footer",
          "className": "bg-dark",
          "attributes": {
            "textOverColor": "dark"
          }
        }
      ]
    },
    "side-panel": {
      "type": "template",
      "label": "Side Panel",
      "description": "template for admin",
      "icon": "template",
      "image": "template.jpg",
      "className": "h-100 w-100",
      "style": {
        "position": "absolute"
      },
      "useGrid": {
        "row": "gx-0 h-100",
        "columns": [
          {
            "className": "col-auto h-100",
            "style": {
              "zIndex": 3
            }
          },
          {
            "className": "h-100",
            "style": {
              "width": 0
            }
          }
        ]
      }
    }
  },
  "pages": {
    "nav-main": {
      "type": "page",
      "label": "Bienvenido",
      "path": "/",
      "icon": "catalog",
      "image": "catalog.jpg",
      "className": "p-2",
      "content": [
        {
          "ref": "$sections.nav-cards",
          "attributes": {
            "menu": "$menus.main"
          }
        }
      ]
    },
    "nav-config": {
      "type": "page",
      "label": "Configuración",
      "path": "/configuracion",
      "icon": "gear",
      "image": "config.jpg",
